{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "eslint-config-airbnb-typescript/base"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 12,
        "project": "./tsconfig.json"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
    },
    "overrides": [
        {
          "files": ["*.ts"],
          "rules": {
            "import/no-cycle": "off",
            "class-methods-use-this": "off",
            "no-plusplus": "off"
          }
        }
      ]
}
